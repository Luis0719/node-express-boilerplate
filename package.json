{
  "name": "express-template",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=16.14.2"
  },
  "scripts": {
    "start": "node ./bin/www",
    "start:debug": "DEBUG=express-template:* npm start",
    "start:debug:all": "DEBUG=*:* npm start",
    "watch": "nodemon ./bin/www",
    "eslint": "eslint src/**/* --fix",
    "eslint-dry": "eslint src/**/*",
    "prettier": "pretty-quick",
    "prettier:all": "prettier --write .",
    "prepare": "husky install",
    "test": "cross-env NODE_ENV=test jest --config=jest.json",
    "pretest": "cross-env NODE_ENV=test npm run migrate",
    "migrate": "npx sequelize-cli db:migrate",
    "unmigrate": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "unseed": "npx sequelize-cli db:seed:undo"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-request-id": "^2.0.1",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "http-errors": "~1.6.3",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "require-directory": "^2.1.1",
    "sequelize": "^6.19.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.4.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.2"
  }
}
