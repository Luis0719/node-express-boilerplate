{
  "name": "express-template",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=16.14.2"
  },
  "scripts": {
    "start": "node ./bin/www",
    "start:debug": "DEBUG=express-template:* npm start",
    "start:debug:all": "DEBUG=*:* npm start",
    "watch": "nodemon ./bin/www",
    "eslint": "eslint src/**/* --fix",
    "eslint-dry": "eslint src/**/*",
    "prettier": "pretty-quick",
    "prettier:all": "prettier --write .",
    "prepare": "husky install",
    "coverage": "cross-env NODE_ENV=test jest --config=jest.json --collect-coverage",
    "test": "cross-env NODE_ENV=test jest --config=jest.json",
    "pretest": "",
    "migrate": "npx sequelize-cli db:migrate",
    "unmigrate": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "unseed": "npx sequelize-cli db:seed:undo"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "require-directory": "^2.1.1",
    "sequelize": "^6.19.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "eslint": "^8.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.2"
  }
}
